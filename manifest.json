{
  "manifest_version": 3,
  "name": "AutoLinked",
  "version": "1.0",
  "action": {
    "default_popup": "./pages/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/contentScript.js"]
    }
  ],
  "permissions": [
    "tabs","storage","background"
  ]
}